public class Pumpkin {
  private float xPos;
  private float yPos;
  
 public Pumpkin(float xPos, float yPos){
   this.xPos = xPos;
   this.yPos = yPos;
 }
 
 public void setYPos(float yPos){
   this.yPos = yPos;
 }
 
 public float getXPos(){
  return this.xPos;
 }
 
 public float getYPos(){
  return this.yPos; 
 }
}
